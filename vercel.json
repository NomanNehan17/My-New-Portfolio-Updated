{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "MY_ENV_VARIABLE": "your_value_here",
    "ANOTHER_VARIABLE": "another_value_here"
  },
  "functions": {
    "index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "regions": [
    "all"
  ]
}
